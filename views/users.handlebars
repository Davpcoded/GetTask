<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h2>Welcome <span class="member-name"></span></h2>
    </div>
  </div>
</div>
<div class="container">
  <div class="section">
    <div class="row">
      <div class="col s12 m2">
      </div>
      <div class="col s12 m8">
        <h5>UserList</h5>
        </br>
        </br>
        <ul class="collection">
          {{#each users}}
          <li class="collection-item avatar">
            <i class="material-icons circle"></i>
            <p>{{this.firstName}} {{this.lastName}}<br>
              Created Tasks: {{this.ticketsCreated}} <br>
              Completed Tasks: {{this.ticketsAssignedClosed}} <br>
            </p>
            <p class="secondary-content">
              Open Tasks: {{this.ticketsAssignedOpen}}
            </p>
          </li>
          {{/each}}
        </ul>
        </br>
        </br>
      </div>
      <div class="col s12 m2">
      </div>
    </div>
  </div>
</div>