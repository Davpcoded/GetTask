<div class="container">
  <div class="section">
    <div class="row">
      <div class="col s12 m2">
      </div>
      <div class="col s12 m8">
        <h5>TaskName</h5>
        </br>
        </br>
        <ul class="collection">
          {{#each tasks}}
          {{#unless completed}}
          <li class="collection-item avatar">
            <i class="material-icons circle"></i>
            <p>{{this.id}}<br>
              {{this.title}} <br>
            </p>
            <p class="secondary-content">
              Completion date: {{this.estCompletion}}
            </p>
            <!-- <a href="#!" class="secondary-content"><i class="material-icons">grade</i></a> -->
          </li>

          {{/unless}}
          {{/each}}
        </ul>
        </br>
        </br>
      </div>
      <div class="col s12 m2">
      </div>
    </div>
  </div>
</div>